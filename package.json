{
  "name": "@shinhyojeong/eslint-plugin",
  "version": "0.0.5",
  "description": "Lint shareable plugin",
  "repository": "shinhyojeong/lint-config",
  "author": "shinyojeong@naver.com",
  "license": "MIT",
  "main": "./lib/index.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "src"
  ],
  "exports": {
    ".": {
      "source": "./lib/index.js",
      "require": "./lib/index.js",
      "import": "./lib/index.mjs"
    }
  },
  "scripts": {
    "lint": "eslint --fix",
    "build": "vite build",
    "changeset": "changeset",
    "version": "changeset version",
    "publish": "yarn build && yarn changeset publish"
  },
  "peerDependencies": {
    "eslint": "^8.52.0",
    "react": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.1",
    "@types/react": "^18.2.35",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  }
}
